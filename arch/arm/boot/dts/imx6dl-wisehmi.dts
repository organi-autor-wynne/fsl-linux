/dts-v1/;

#include "imx6dl.dtsi"
#include "imx6qdl-wisehmi.dtsi"

/ {
	model = "Freescale i.MX6 Quad/Dual Wise HMI Board";
	compatible = "autorock,imx6dl-wisehmi", "autorock,imx6qdl-wisehmi", "fsl,imx6dl";
};

&lcd {
	status = "okay";
};

&ldb {
	status = "disabled";
};

&mxcfb1 {
	disp_dev = "lcd";
	status = "okay";
};

&fec {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&uart4 {
	status = "okay";
};

&usdhc2 {
	status = "okay";
};

&usdhc3 {
	status = "okay";
};

/*&flexcan1 {
	status = "okay";
};

&flexcan2 {
	status = "okay";
};*/

&usbh1 {
	status = "okay";
};


